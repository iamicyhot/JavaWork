package javademo;

public class TwoDArray {

	public static void main(String[] args) {
		int new2[][] = new int[3][2];
		for(int i=0; i<3; i++)
		{
			for(int j=0; j<2; j++)
			{
				new2[i][j]= ((i*10)+j);
			}
		}
		for(int i=0; i<3; i++)
		{
			System.out.println();
			for(int j=0; j<2; j++)
			{
				System.out.print("  "+new2[i][j]);
			}
		}
			

	}

}
